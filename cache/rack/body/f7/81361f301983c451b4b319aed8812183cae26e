    <script type='text/javascript'>
      //<![CDATA[
        $(function() {
            var offset = $("#item").offset();
            var topPadding = 0;
            $(window).scroll(function() {
                if ($(window).scrollTop() > offset.top) {
                    $("#item").stop().animate({
                        marginTop: $(window).scrollTop() - offset.top + topPadding
                    });
                } else {
                    $("#item").stop().animate({
                        marginTop: 0
                    });
                };
            });
        });
        $(document).ready(function(){
          $('.title').mouseover(function(){
            var html = $(this).html();
            html += $($(this).next()).html();
            $('#item').html(html);
          })
        });
      //]]>
